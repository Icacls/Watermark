{
  "name": "page-watermark",
  "version": "1.0.8",
  "description": "Watermark is a simple and quick library to draw watermark on HTML element.",
  "main": "dist/Watermark.min.js",
  "license": "MIT",
  "scripts": {
    "start": "webpack --config webpack.dev.js --watch",
    "build": "webpack --config webpack.prod.js",
    "lint": "eslint src",
    "test": "jest",
    "prebuild": "yarn lint && yarn test",
    "postbuild": "rm -rf docs/dist && cp -r dist docs/dist"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "keywords": [
    "watermark"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/YanxinTang/Watermark.git"
  },
  "author": "winicafe",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.6.3",
    "autoprefixer": "^9.7.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "canvas": "^2.6.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-environment-jsdom-fourteen": "^1.0.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9"
  },
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "core-js": "3.6.0"
  }
